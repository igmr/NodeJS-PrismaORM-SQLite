{
    "editor.fontSize": 16,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "editor.renderWhitespace": "all",
    "sqltools.useNodeRuntime": true,
    "sqltools.connections": [
        {
            "previewLimit": 50,
            "driver": "SQLite",
            "database": "${workspaceFolder:NodeJS-PrismaORM-SQLite3}/src/database/dev.db",
            "name": "NodeJS-PrimaORM-SQLite"
        }
    ],
}

